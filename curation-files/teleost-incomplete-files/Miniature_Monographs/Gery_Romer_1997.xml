      <char id="c7a357ecc-63fb-4c8f-8fd6-92a8a66dfdea" label="No sexual hooks on pelvic, anal or caudal fins rays" states="scf949bd1-8f61-4ce9-bac7-6a8a1e857824" />
      <char id="cab88f136-f86e-4231-b8da-f1ffc792a20e" label="No caudal gland" states="sc0a06859-db22-4618-a512-dcc506c29174" />
      <char id="c633f77a5-44ed-4071-aab2-dd7859a97a66" label="No frontal organ (as in the Crenuchidae)" states="s1d2a00d4-df91-4405-bd9b-50a85730b380" />
